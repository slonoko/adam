# Alpha Vantage API Key for stock market data
# Get your free API key at: https://www.alphavantage.co/support/#api-key
ALPHAVANTAGE_KEY=your_alphavantage_api_key_here

# Exchange Rate API Key for currency conversion
# Get your free API key at: https://exchangerate-api.com/
EXCHANGE_RATE_API_KEY=your_exchange_rate_api_key_here

# News API Key for news aggregation
# Get your free API key at: https://newsapi.org/
NEWS_API_KEY=your_news_api_key_here

# Google Cloud Configuration
# Project ID for your Google Cloud project
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id

# Google Cloud region/location for services
GOOGLE_CLOUD_LOCATION=us-central1

# Google API Key for Generative AI services
# Get your API key from: https://console.cloud.google.com/apis/credentials
GOOGLE_API_KEY=your_google_api_key_here

# Use Vertex AI for Google Generative AI (set to True for production)
GOOGLE_GENAI_USE_VERTEXAI=True